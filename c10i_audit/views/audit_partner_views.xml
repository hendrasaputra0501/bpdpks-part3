<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="audit_partner_view_tree" model="ir.ui.view">
        <field name="name">audit.partner.view.tree</field>
        <field name="model">audit.partner</field>
        <field name="arch" type="xml">
            <tree string="Auditi">
                <field name="name" />
                <field name="alamat" />
                <field name="phone" />
                <field name="email" />
            </tree>
        </field>
    </record>

    <record id="audit_partner_view_form" model="ir.ui.view">
        <field name="name">audit.partner.view.form</field>
        <field name="model">audit.partner</field>
        <field name="arch" type="xml">
            <form string="Auditi">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="alamat" />
                        <field name="phone" />
                        <field name="email" />
                        <field name="contact_person" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="audit_partner_action" model="ir.actions.act_window">
        <field name="name">Audit partner</field>
        <field name="res_model">audit.partner</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>

</odoo>
